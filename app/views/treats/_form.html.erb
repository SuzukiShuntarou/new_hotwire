 <%= turbo_frame_tag treat do %>
  <%= bootstrap_form_with(model: treat, data: { action: "turbo:submit-end->modal#close" }) do |form| %>
      <%= form.date_field :completiondate, value: treat.completiondate %>
      <%= form.text_area :location %>
      <%= form.text_area :content %>
      <%= form.submit %>
  <% end %>
<% end %>
