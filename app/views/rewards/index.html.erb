<p style="color: green"><%= notice %></p>

<% @rewards.each do |reward| %>
  <%= turbo_frame_tag reward do %>
  作成者：<%= reward.user.email %>
  <br />
  <%= reward.completiondate %>に　
  <%= reward.location %>で　
  <%= reward.content %>する
  <%= link_to "編集", edit_reward_path(reward) %>
  <%= button_to "削除", reward, method: :delete %>
  <br />
  <br />
  <% end %>
<% end %>

<%= paginate @rewards %>
<br />
<%= link_to "新規作成", new_reward_path %>
